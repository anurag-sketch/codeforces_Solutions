#include<bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	int arr[n];
    
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int count=0;
    for(int i=0;i<n;i++)
    {
        if(arr[i]==1)
        {
            cout<<"HARD"<<endl;
            break;
            
        }
        count++;
    }
    if(count==n)
    {
        cout<<"EASY"<<endl;
    }
    
	 
	return 0;
}
